                <fs xml:id="me" feats="#modern #p1 #acusative #unaccented #not_assim"/>
                <fs xml:id="m_" feats="#p1 #acusative #unaccented #not_assim #not_vowel"/>
                <!-- The variation in these last three is related to the grammatical function and not to the form itself, 
                    so you won't be able to recognise which one is the correct one. Choose one randomly, and I'll review it later-->
                <fs xml:id="te_dative" feats="#p2 #dative #modern"/>
                <fs xml:id="te_acusative" feats="#p2 #acusative #modern"/>
                <fs xml:id="mi_acusative" feats="#p1 #archaic"/>
