                <fs xml:id="contigo" feats="#accented #p2 #comitative #modern"/>
                <fs xml:id="consigo" feats="#accented #p3p6 #comitative #modern"/>
                <fs xml:id="connosco" feats="#accented #p4 #comitative #modern"/>
